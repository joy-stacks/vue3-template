{
  "name": "@zjb/template",
  "private": false,
  "title": "千伏安框架",
  "description": "输血协会前端项目框架",
  "version": "1.0.1",
  "type": "module",
  "scripts": {
    "bootstrap": "yarn install",
    "serve": "npm run dev",
    "dev": "vite",
    "build": "rimraf dist && vue-tsc --noEmit --skipLibCheck  && vite build && esno ./build/scripts/build.ts",
    "build:no-cache": "yarn clean:cache && npm run build",
    "report": "cross-env REPORT=true npm run build",
    "preview": "npm run build && vite preview",
    "preview:dist": "vite preview",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "clean:lib": "rimraf node_modules",
    "build typecheck": "vuedx-typecheck . && vite build",
    "deploy": "gh-pages -d dist",
    "lint:eslint": "eslint \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.js",
    "lint:pretty": "pretty-quick --staged",
    "test prod gzip": "http-server dist --cors --gzip -c-1"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.1.0",
    "@zjb/components": "^2.0.7",
    "@zjb/utils": "^1.0.2",
    "ant-design-vue": "^3.2.15",
    "dayjs": "^1.11.7",
    "makeit-captcha": "^2.0.6",
    "mitt": "^3.0.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.33",
    "pinia-plugin-persist": "^1.0.0",
    "print-js": "^1.6.0",
    "vue": "^3.2.47",
    "vue-draggable-next": "^2.1.1",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.15.3",
    "@types/nprogress": "^0.2.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "chalk": "^5.2.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.36.0",
    "esno": "^0.16.3",
    "fs-extra": "^11.1.0",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "rimraf": "^4.4.1",
    "rollup-plugin-visualizer": "^5.9.0",
    "typescript": "^4.9.3",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.1.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue-tsc": "^1.0.24"
  },
  "author": "Blood transfusion association team",
  "license": "MIT",
  "publishConfig": {
    "registry": "http://192.168.200.86:8888/"
  }
}
